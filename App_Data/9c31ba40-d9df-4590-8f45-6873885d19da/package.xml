	<Actions>
		<Action runat="install" alias="addDashboardSection" dashboardAlias="ExportSqlCE">
			<section alias="ExportSqlCE">
				<areas>
					<area>developer</area>
				</areas>
				<tab caption="Export SQL Server Compact">
					<control addPanel="true" panelCaption="">/umbraco/plugins/ExportSqlCE/Dashboard.ascx</control>
				</tab>
			</section>
		</Action>
	</Actions>
	<control />
